<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <PCL_DIR>C:\Program Files (x86)\PCL\include\pcl-1.8\</PCL_DIR>
    <PCL_LIB>C:\Program Files (x86)\PCL\lib</PCL_LIB>
    <LIBS_DIR>C:\Libs</LIBS_DIR>
    <BOOST_DIR>$(LIBS_DIR)/boost/include</BOOST_DIR>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="PCL_DIR">
      <Value>$(PCL_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="PCL_LIB">
      <Value>$(PCL_LIB)</Value>
    </BuildMacro>
    <BuildMacro Include="LIBS_DIR">
      <Value>$(LIBS_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="BOOST_DIR">
      <Value>$(BOOST_DIR)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>